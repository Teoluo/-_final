	<div class="container container-small">

		<h1>登陆 </h1>
		<form id="mySignInForm" action="" method="POST">
			<div class="form-group">
				<label>用户名</label>
				<input type="text" name="userName" class="form-control">
			</div>
			<div class="form-group">
				<label>密码</label>
				<input type="password" name="password" class="form-control">
			</div>
			<div class="form-group">
				<button class="btn btn-primary btn-block" type="submit" id="jqSubmit">登陆</button>
			</div>

			<label>
				<small>没有账号？
					<a href="signup">去注册</a>
				</small>
			</label>
		</form>
	</div>

	<script src="js/jquery.js"></script>
	<script>
		$.ajax({
			type: 'POST',
			dataType: 'json',
			url: '/signin',
			data: $('#mySignInForm').serialize(),
			success: function (data) {
			}
		});
	</script>

</body>

