    <title>天天炉石</title>
    <link rel="stylesheet" href="/css/bootstrap.css">
    <link rel="stylesheet" href="/css/user.css">
</head>

<body>
    <nav class="navbar navbar-expand-sm bg-light navbar-light  justify-content-between" id="navbar-first">
        <a class="navbar-brand" href="index">天天炉石</a>
        <ul class="nav navbar-nav">

            <li class="nav-item">
                <a class="nav-link" href="#">Teoluo</a>
            </li>
        </ul>
    </nav>
    <nav class="navbar navbar-expand-sm bg-light navbar-light  justify-content-center" id="navbar-item">
        <ul class="nav navbar-nav justify-content-end">
            <li class="nav-item">
                <a class="nav-link" href="sort/de">德鲁伊</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="sort/lie">猎人</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="sort/fa">法师</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="sort/shen">圣骑士</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="sort/mu">牧师</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="sort/qian">潜行者</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="sort/sa">萨满</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="sort/shu">术士</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="sort/zhan">战士</a>
            </li>
        </ul>
    </nav>


    <div class="content">
        <div>
            <div class="row">
                <div class="col-sm-2">
                    <div class="nav flex-column nav-pills" id="v-pills-tab" role="tablist" aria-orientation="vertical">
                        <a class="nav-link active" id="v-pills-home-tab" data-toggle="pill" href="#v-pills-home" role="tab" aria-controls="v-pills-home"
                            aria-selected="true">个人信息</a>
                        <a class="nav-link" id="v-pills-profile-tab" data-toggle="pill" href="#v-pills-profile" role="tab" aria-controls="v-pills-profile"
                            aria-selected="false">修改资料</a>
                        <a class="nav-link" id="v-pills-messages-tab" data-toggle="pill" href="#v-pills-messages" role="tab" aria-controls="v-pills-messages"
                            aria-selected="false">新建文章</a>
                        <!-- <a class="nav-link" id="v-pills-settings-tab" data-toggle="pill" href="#v-pills-settings" role="tab" aria-controls="v-pills-settings"
                            aria-selected="false">管理文章</a> -->
                    </div>
                </div>
                <div class="tab-content col-sm-10 " id="v-pills-tabContent">
                    <!-- 个人信息的展示 -->
                    <div class="tab-pane fade show active" id="v-pills-home" role="tabpanel" aria-labelledby="v-pills-home-tab">
                        <div class="usrInfo">
                            <div>
                                <h3>用户名</h3>
                            </div>
                            <div style='margin-left: 30px'>
                                <p>Teoluo</p>
                            </div>
                            <div>
                                <h3>手机号</h3>
                            </div>
                            <div style='margin-left: 30px'>
                                <p>12345678910</p>
                            </div>
                        </div>


                    </div>
                    <!-- 修改信息 -->
                    <div class="tab-pane fade" id="v-pills-profile" role="tabpanel" aria-labelledby="v-pills-profile-tab">
                        <form action="" id="submitInfo">
                            <div class="form-group">
                                <label>用户名：</label>
                                <input type="text" name="userName" class="form-control">
                            </div>
                            <div class="form-group">
                                <label>密码：</label>
                                <input type="password" name="password" class="form-control">
                            </div>
                            <div class="form-group">
                                <label>手机号：</label>
                                <input type="text" name="phone" class="form-control">
                            </div>
                            <div>
                                <button class="btn btn-primary" type="submit">提交</button>
                            </div>
                        </form>
                    </div>

                    <!-- 新建文章 -->
                    <div class="tab-pane fade" id="v-pills-messages" role="tabpanel" aria-labelledby="v-pills-messages-tab">
                        <form action="" id="submitArticle">

                            <div class="form-group">
                                <label>标题：</label>
                                <input type="text" class="form-control" name="title">
                            </div>
                            <div class="form-group">
                                <label>作者：</label>
                                <input type="text" class="form-control" name="author">
                            </div>
                            <div class="form-group">
                                <label>标签：</label>
                                <input type="text" class="form-control" name="tag">
                            </div>
                            <div class="form-group">
                                <label>摘要：</label>
                                <input type="text" class="form-control" name="summary">
                            </div>
                            <div>
                                <div id="editor">
                                    <p></p>
                                </div>
                            </div>
                            <button id="btn1" class="btn btn-primary" type="submit">提交</button>
                        </form>
                    </div>
                    <div class="tab-pane fade" id="v-pills-settings" role="tabpanel" aria-labelledby="v-pills-settings-tab">
                        管理文章
                    </div>
                </div>
            </div>
        </div>
    </div>


    <script src="/js/jquery.js"></script>
    <script src="/js/popper.min.js"></script>
    <script src="/js/bootstrap.js"></script>
    <script type="text/javascript" src="https://cdn.bootcss.com/wangEditor/10.0.13/wangEditor.min.js"></script>
    <script type="text/javascript">
        var E = window.wangEditor
        var editor1 = new E('#editor')
        editor1.create()
        console.log('有用吗')
        console.log(editor1.txt.html())
        document.getElementById('btn1').addEventListener('click', function () {
            // 读取 html
            console.log(editor1.txt.html())
        }, false)

        $.ajax({
            type: 'POST',
            dataType: 'json',
            url: '/signin',
            data: $('#submitArticle').serialize(),
            // data:{
            //     title:'',
            //     author:'',
            //     tag:'',
            //     summary:'',
            //     body:'',
            //     date: new Date(),
            // },
            success: function (data) {
                console.log(data)
            }
        });
    </script>
</body>

</html>